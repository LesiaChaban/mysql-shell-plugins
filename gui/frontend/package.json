{
    "name": "mysql-shell-gui",
    "version": "1.0.0",
    "publisher": "Oracle",
    "dependencies": {
        "ansi-to-react": "6.1.6",
        "antlr4-c3": "2.2.1",
        "antlr4ts": "0.5.0-alpha.4",
        "classnames": "2.3.1",
        "color": "3.2.1",
        "d3": "7.1.1",
        "idb": "6.1.5",
        "keyboard-key": "1.1.0",
        "monaco-editor": "0.30.1",
        "pixi-ease": "3.0.7",
        "pixi-scrollbox": "2.3.1",
        "pixi-viewport": "4.34.4",
        "pixi.js": "6.2.2",
        "preact": "10.6.6",
        "react-monaco-editor": "0.46.0",
        "react-window": "1.8.6",
        "reconnecting-websocket": "4.4.0",
        "set-interval-async": "2.0.3",
        "tabulator-tables": "5.1.0",
        "topojson-client": "3.1.0",
        "ws": "8.5.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.14.1",
        "@testing-library/preact": "2.0.1",
        "@testing-library/react": "12.1.2",
        "@types/color": "3.0.2",
        "@types/d3": "7.1.0",
        "@types/enzyme": "3.10.10",
        "@types/jest": "27.0.3",
        "@types/jsdom": "16.2.13",
        "@types/lodash": "4.14.177",
        "@types/node": "16.11.11",
        "@types/react-window": "1.8.5",
        "@types/resize-observer-browser": "0.1.6",
        "@types/selenium-webdriver": "4.0.16",
        "@types/set-interval-async": "1.0.0",
        "@types/tabulator-tables": "5.1.0",
        "@types/topojson-client": "3.1.0",
        "@types/ws": "8.2.1",
        "@typescript-eslint/eslint-plugin": "5.5.0",
        "@typescript-eslint/eslint-plugin-tslint": "5.5.0",
        "acorn": "8.6.0",
        "antlr4ts-cli": "0.5.0-alpha.4",
        "enzyme": "3.11.0",
        "enzyme-adapter-preact-pure": "3.3.0",
        "enzyme-to-json": "3.6.2",
        "eslint": "7.32.0",
        "eslint-config-preact": "1.2.0",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-jsdoc": "37.1.0",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-react": "7.27.1",
        "fake-indexeddb": "3.1.7",
        "identity-obj-proxy": "3.0.0",
        "jest-canvas-mock": "2.3.1",
        "monaco-editor-webpack-plugin": "6.0.0",
        "monaco-typescript": "4.10.0",
        "mysql-promise": "5.0.0",
        "oci-bastion": "2.9.0",
        "oci-common": "2.9.0",
        "oci-core": "2.9.0",
        "oci-identity": "2.9.0",
        "oci-loadbalancer": "2.9.0",
        "oci-mysql": "2.9.0",
        "raw-loader": "4.0.2",
        "react-app-rewired": "2.1.8",
        "react-scripts": "3.4.4",
        "run-my-sql-file": "1.0.0",
        "selenium-webdriver": "4.1.0",
        "typescript": "3.9.10",
        "typescript-eslint-parser": "22.0.0",
        "utf-8-validate": "5.0.7",
        "webdriver-manager": "12.1.8",
        "worker-loader": "3.0.8"
    },
    "scripts": {
        "launch-shell-web-server": "bash ./scripts/launch-shell-webserver.sh",
        "launch-dev-server": "bash ./scripts/launch-dev-server.sh 3001",
        "launch-dev-server-win": "set PORT=3001&&set HTTPS=true&&call scripts/launch-dev-server.cmd",

        "build": "bash ./scripts/production-build.sh false",
        "build-with-maps": "bash ./scripts/production-build.sh true",
        "build-win": "call scripts/production-build.cmd false",
        "build-win-with-maps": "call scripts/production-build.cmd true",

        "update-snapshots": "react-scripts test --testMatch [ \"**/tests/unit-tests/**/*.spec.[jt]s?(x)\" ] --watchAll=false -u",
        "test-local": "react-scripts test --testMatch [ \"**/tests/unit-tests/**/*.spec.[jt]s?(x)\" ] --coverage --watchAll=false --maxWorkers=1",
        "test": "react-scripts test --testMatch [ \"**/tests/unit-tests/**/*.spec.[jt]s?(x)\" ] --coverage --watchAll=false",
        "lint": "eslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",

        "e2e": "react-scripts test --detectOpenHandles --testMatch [ \"**/tests/e2e/**/*.spec.[jt]s?(x)\" ] --setupFilesAfterEnv \"./src/tests/e2e/setupTests.js\" --coverage --watchAll=false",
        "e2e-update": "webdriver-manager update --out_dir ../../src/tests/e2e/selenium",
        "e2e-start": "webdriver-manager start --out_dir ../../src/tests/e2e/selenium"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "jest": {
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!**/node_modules/**",
            "!src/app-wrapper/**",
            "!src/assets/**",
            "!src/parsing/mysql/generated/**",
            "!src/parsing/SQLite/generated/**",
            "!src/parsing/python/generated/**",
            "!src/**/*.d.ts"
        ],
        "coverageReporters": [
            "json",
            "text",
            "clover",
            "html"
        ],
        "coverageThreshold": {
            "global": {
                "statements": 20,
                "branch": 13,
                "functions": 22,
                "lines": 20
            }
        },
        "transformIgnorePatterns": [
            "/node_modules/(?!monaco-editor)/",
            "/node_modules/(?!tabulator-tables)/"
        ],
        "moduleNameMapper": {
            "^monaco-editor$": "monaco-editor/esm/vs/editor/editor.api",
            "^tabulator-tables$": "tabulator-tables/dist/js/tabulator_esm.js",
            "^.+\\.worker$": "<rootDir>/src/tests/unit-tests/__mocks__/workerMock.ts",
            "^.+runtime\\.d\\.ts$": "<rootDir>/src/tests/unit-tests/__mocks__/typingsMock.ts",
            "^react$": "preact/compat",
            "^react-dom/test-utils$": "preact/test-utils",
            "^react-dom$": "preact/compat",
            "^react/jsx-runtime$": "preact/jsx-runtime"
        }
    }
}
